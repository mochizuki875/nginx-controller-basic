domain: my.domain
layout:
- go.kubebuilder.io/v3
projectName: nginx-controller
repo: example.com/nginx-controller
resources:
- api:
    crdVersion: v1
    namespaced: true
  controller: true
  domain: my.domain
  group: nginx
  kind: Nginx
  path: example.com/nginx-controller/api/v1
  version: v1
version: "3"
